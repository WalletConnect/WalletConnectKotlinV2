<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ConnectButton">
        <attr name="connect_button_size" />
    </declare-styleable>

    <declare-styleable name="AccountButton">
        <attr name="account_button_type" />
    </declare-styleable>

    <declare-styleable name="Web3Button">
        <attr name="account_button_type" />
        <attr name="connect_button_size" />
    </declare-styleable>

    <attr name="connect_button_size" format="enum">
        <enum name="NORMAL" value="0" />
        <enum name="SMALL" value="1" />
    </attr>

    <attr name="account_button_type" format="enum">
        <enum name="NORMAL" value="0" />
        <enum name="MIXED" value="1" />
    </attr>
</resources>